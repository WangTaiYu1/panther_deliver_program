<launch>

    <node pkg = 'move_base' type = 'move_base' respawn = 'false' name='move_base' output = 'screen'>
        <rosparam file = '$(find demo)/config/costmap_common_params.yaml' command = 'load' ns = 'global_map' />
        <rosparam file = '$(find demo)/config/costmap_common_params.yaml' command = 'load' ns = 'local_map' />
        <rosparam file = '$(find demo)/config/local_costmap_params.yaml' command = 'load' ns = 'global_map' />
        <rosparam file = '$(find demo)/config/global_costmap_params.yaml' command = 'load' ns = 'global_map' />
        <rosparam file = '$(find demo)/config/trajectory_planner.yaml' command = 'load' ns = 'global_map' />
    </node>


</launch>